{
  "name": "component-x",
  "version": "0.1.0",
  "description": "Component package manager consuming git repositories",
  "keywords": [
    "component",
    "package",
    "client",
    "browser",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/component/component.git"
  },
  "dependencies": {
    "commander": "1.0.5",
    "mkdirp": "0.3.4",
    "request": "~2.49.0",
    "component-builder-x": "0.1.3",
    "string-to-js": "0.0.1",
    "batch": "0.5.0",
    "win-fork": "1.0.0",
    "archy": "0.0.2",
    "netrc": "~0.1.3",
    "debug": "*",
    "rimraf": "~2.2.8",
    "open": "0.0.4"
  },
  "devDependencies": {
    "express": "3.0.0",
    "minstache": "0.0.1",
    "mocha": "*",
    "should": "*"
  },
  "bin": {
    "component": "bin/component",
    "component-info": "bin/component-info",
    "component-help": "bin/component-help",
    "component-install": "bin/component-install",
    "component-ls": "bin/component-ls",
    "component-convert": "bin/component-convert",
    "component-build": "bin/component-build",
    "component-search": "bin/component-search",
    "component-create": "bin/component-create",
    "component-changes": "bin/component-changes",
    "component-wiki": "bin/component-wiki"
  },
  "scripts": {
  	"test": "./node_modules/.bin/mocha --require should --timeout 90000"
  },
  "main": "index",
  "engines": {
    "node": ">= 0.8.0"
  },
  "bugs": {
    "url": "https://github.com/Wizcorp/component-x/issues"
  }
}
